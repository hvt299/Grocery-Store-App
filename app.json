{
  "expo": {
    "name": "Grocery Store App",
    "slug": "GroceryStoreApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.huathai.GroceryStoreApp",
      "softwareKeyboardLayoutMode": "pan",
      "userInterfaceStyle": "light"
    },
    "web": {
      "favicon": "./assets/logo-app.png"
    },
    "extra": {
      "eas": {
        "projectId": "93754d5f-7799-4038-a991-ab83173c8a05"
      }
    },
    "plugins": [
      "expo-font",
      "expo-navigation-bar",
      {
        "barStyle": "dark-content",
        "backgroundColor": "#FFFFFF"
      }
    ]
  }
}